{
  "name": "@nuxtjs/robots",
  "version": "3.0.0",
  "description": "A Nuxt.js module that injects a middleware to generate a robots.txt file",
  "repository": "nuxt-community/robots-module",
  "license": "MIT",
  "contributors": [
    "Ricardo Gobbo de Souza <ricardogobbosouza@yahoo.com.br>"
  ],
  "exports": {
    ".": {
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev": "nuxt dev playground",
    "lint": "eslint",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm build && changelogen --release && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && vitest run --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "h3": "^1.0.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "latest",
    "@vitest/coverage-c8": "latest",
    "changelogen": "latest",
    "eslint": "9.1.1",
    "nuxt": "latest",
    "vitest": "latest",
    "@nuxt/eslint-config": "0.3.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.8.0"
}